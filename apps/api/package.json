{
  "name": "@regintel/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "worker": "tsx watch src/worker.ts",
    "lint": "eslint src --ext .ts",
    "db:push": "prisma db push --schema=../../packages/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=../../packages/database/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=../../packages/database/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=../../packages/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@esbuild/darwin-arm64": "^0.25.10",
    "@fastify/cors": "^10.0.1",
    "@fastify/env": "^5.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@regintel/database": "*",
    "@regintel/shared": "*",
    "bullmq": "^5.31.2",
    "cheerio": "^1.1.2",
    "fastify": "^5.2.1",
    "ioredis": "^5.4.2",
    "pdf-parse": "^2.1.3",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22",
    "eslint": "^9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
